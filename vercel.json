{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://sos-voz-feminina-backend.onrender.com/api",
    "VITE_APP_NAME": "SOS Voz Feminina",
    "VITE_APP_VERSION": "1.0.0"
  }
}
